# problem domain setup
-Dim 2 -Mx 200 -My 200

# problem parameter setup
-ft 0.001 -dt 0.01 -eta 1.0

# problem type
-EquationType Stokes

# linear solver setup
-log_view
-ksp_type fgmres -ksp_monitor_true_residual -ksp_gmres_restart 100

-pc_type fieldsplit -pc_fieldsplit_type schur -pc_fieldsplit_schur_fact_type lower
-fieldsplit_0_pc_type hypre
-fieldsplit_1_pc_type hypre
-fieldsplit_0_pc_hypre_type boomeramg -fieldsplit_0_pc_hypre_boomeramg_interp_type classical -fieldsplit_0_pc_hypre_boomeramg_coarsen_type Falgout -fieldsplit_0_pc_hypre_boomeramg_relax_type_down l1-Gauss-Seidel -fieldsplit_0_pc_hypre_boomeramg_relax_type_up backward-l1-Gauss-Seidel -fieldsplit_0_pc_hypre_boomeramg_relax_type_coarse l1scaled-Jacobi
-fieldsplit_1_pc_hypre_type boomeramg -fieldsplit_1_pc_hypre_boomeramg_interp_type direct -fieldsplit_1_pc_hypre_boomeramg_coarsen_type Ruge-Stueben -fieldsplit_1_pc_hypre_boomeramg_relax_type_down l1-Gauss-Seidel -fieldsplit_1_pc_hypre_boomeramg_relax_type_up backward-l1-Gauss-Seidel -fieldsplit_1_pc_hypre_boomeramg_relax_type_coarse l1scaled-Jacobi
-fieldsplit_0_ksp_type preonly -fieldsplit_0_ksp_max_it 1
-fieldsplit_1_ksp_type preonly -fieldsplit_1_ksp_max_it 1

# rigid body inclusion
-rigid_body_input example/2d_rigid_body.in